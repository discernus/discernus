# Environment Configuration for Discernus Analysis Platform
# Copy this to .env and update the values for your setup

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/discernus

# Debug Settings
SQL_DEBUG=false

# API Keys (Hugging Face integration now active)
HUGGINGFACE_API_KEY=your_api_key_here

# Task Queue Configuration (Celery integration now active)
REDIS_URL=redis://localhost:6379/0

# Security Configuration (JWT Authentication now active)
SECRET_KEY=your_secret_key_here_for_general_security
JWT_SECRET_KEY=your_jwt_secret_key_here_for_token_signing 